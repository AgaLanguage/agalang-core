importa ':consola' como Consola;

clase Animal {
  pub sonar(){
    Consola::pintar("Hu");
    esto::mover();
  };
  mover(){
    Consola::pintar("Moviendo...");
  };
  est sonar(){
    Consola::pintar("~~");
  };
  est __constructor__(){
    Consola::pintar("creando un animal");
    ret esto;
  };
}

const animal = Animal();
animal::sonar();