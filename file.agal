#importa ':proto' como PT; #al cargar proto se carga automaticamente proto/Numero para no repetir
importa ':consola' como Consola;
importa ':tmp' como Tmp;

asinc fn funcion(){
  
  espera Tmp::esperar(10);
  Consola::pintar("Adentro");
}

def a = funcion()::luego(fn (){
  Consola::pintar("Adentro luego");
  lanza ":D";
})::atrapa(fn (){
  Consola::pintar("Adentro atrapa");
  ret ":C";
});
Consola::pintar("Afuera");
def b = espera a;
Consola::pintar(b);