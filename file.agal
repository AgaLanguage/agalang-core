importa ':consola' como Consola;

clase Animal {
  pub sonar(){
    Consola::pintar("Hu");
    esto::mover();
  };
  mover(){
    Consola::pintar("Moviendo...");
  };
  est sonar(){
    Consola::pintar("~~");
  };
  est __constructor__(@texto){
    Consola::pintar(texto);
    esto.gorro = Gorro();
    esto; # el ultimo valor se retorna en cualquier funcion
  };
}

const animal = Animal("le puedes","mandar un","argumento");
animal::sonar();